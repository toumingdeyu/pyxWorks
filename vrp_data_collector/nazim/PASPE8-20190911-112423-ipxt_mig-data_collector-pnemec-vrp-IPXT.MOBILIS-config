interface Bundle-Ether1
 description TESTING PASPE4 from PASPE3 :IPXT ASN327712 @XXX.XXX.XXX.XXX - For IPXT over IPSEC FIBLD011706 - Custom
 no ipv4 address
 carrier-delay up 3 down 0
 load-interval 30
!
interface Bundle-Ether1.122
 encapsulation dot1Q 122
 description TESTING MOBILIS :IPXT ASN327712 @dummy_value - IPX LD011706 TunnelIpsec122 - Custom
 bandwidth 200000
 vrf IPXT@MOBILIS 
 ipv4 address dummy_value 255.255.255.254
 flow ipv4 monitor ICX sampler ICX ingress
 ipv4 access-group IPXT.MOBILIS-IN ingress
 service-policy input IPXT.MOBILIS-IN
 service-policy output IPXT.MOBILIS-OUT
!
!
route-policy IPXT.MOBILIS-IN
  if not destination in IPXT.MOBILIS-IN then
   drop
  endif
  if community matches-any (2300:80) then
    set local-preference 80
    set community (2300:11000) 
    set community (2300:20200) additive
  elseif community matches-any (2300:90) then
    set local-preference 90
    set community (2300:11000)
    set community (2300:20200) additive
  else
    set local-preference 100
    set community (2300:11000)
    set community (2300:20200) additive
  endif
end-policy 
!
!
router bgp 2300
 neighbor-group IPXT.MOBILIS
  remote-as 327712
  ebgp-multihop 5
  advertisement-interval 0
  address-family ipv4 unicast
   send-community-ebgp
   route-policy IPXT.MOBILIS-IN in
   maximum-prefix 10 90
   route-policy PASS-ALL out
   soft-reconfiguration inbound
  !
 !
vrf IPXT@MOBILIS
  rd 0.0.0.:122
  address-family ipv4 unicast
   redistribute connected route-policy NO-EXPORT-INTERCO
  !
  neighbor 172.25.10.124
   use neighbor-group IPXT.MOBILIS
  !
!
!
router static
 vrf IPXT@MOBILIS 
  address-family ipv4 unicast
  172.25.10.124/32 Bundle-Ether1.122 dummy_value
!
